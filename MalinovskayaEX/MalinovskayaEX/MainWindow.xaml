        <Frame Name ="_mainFrame" Grid.RowSpan="4" Grid.ColumnSpan="5" Height="379" Width="446"/>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
